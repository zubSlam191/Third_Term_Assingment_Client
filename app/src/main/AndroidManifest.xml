<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zubairslamdien.myapplicationwg">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zubairslamdien.myapplicationwg.MainActivity"
            android:label="Emails">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        android:name="com.zubairslamdien.myapplicationwg.CreateMail"
        android:label="Create Emails" />
        <activity
            android:name="com.zubairslamdien.myapplicationwg.DeleteMail"
            android:label="Delete Emails" />
        <activity
            android:name="com.zubairslamdien.myapplicationwg.SearchMail"
            android:label="Search for Emails" />

    </application>

</manifest>